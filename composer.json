{
    "name": "ectool/amocrm-bundle",
    "description": "Symfony bundle for amoCRM",
    "type": "symfony-bundle",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Ectool\\AmoCrmBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Yuri Peshkin",
            "email": "yuri.peshkin@easycomm.ru"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/config": "^6.4",
        "symfony/console": "^6.4",
        "symfony/dependency-injection": "^6.4",
        "symfony/framework-bundle": "^6.4",
        "symfony/http-client": "^6.4",
        "symfony/http-kernel": "^6.4",
        "symfony/property-access": "^6.4",
        "symfony/serializer": "^6.4",
        "symfony/serializer-pack": "^1.3",
        "amocrm/amocrm-api-library": "^1.10.2"
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.59"
    },
    "scripts": {
        "php-cs-fixer": "php-cs-fixer fix --config=.php-cs-fixer.php"
    }
}
